# Uncomment everything below this line and set your ssid and password
[connection]
id=OctoPiWiFi
uuid=593819b8-135a-4a3e-9611-c36cdeadbeef
type=wifi
autoconnect-priority=-999
interface-name=wlan0

[wifi]
band=bg
channel=3
mode=ap
ssid=OctoPiWiFi

[wifi-security]
key-mgmt=wpa-psk
psk=raspberry

[ipv4]
address1=10.10.10.1/24,10.10.10.1
method=shared

[ipv6]
addr-gen-mode=stable-privacy
method=disabled

[proxy]
